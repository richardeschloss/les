		"sslKey": {
			"desc": "Path to SSL Key",
			"en_US": "sslKey"
		},
		"sslCert": {
			"desc": "Path to SSL Certificate",
			"en_US": "sslCert"
		},
		"open": {
			"alias": "o",
			"desc": "Open browser",
			"dflt": "OS default",
			"en_US": "open"
