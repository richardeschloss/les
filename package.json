{
  "name": "lesky",
  "version": "2.0.0",
  "description": "Lightweight Express-ish (Koa) Server - Type les not More!",
  "main": "server/koa.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/cli.js --watch",
    "start": "server/cli.js",
    "test": "TEST=true ava",
    "test:watch": "TEST=specs ava --watch --timeout=10m",
    "test:cov": "TEST=specs c8 ava --timeout=10m"
  },
  "bin": {
    "les": "./server/cli.js"
  },
  "files": [
    "server",
    "public",
    "test",
    ".editorconfig",
    ".eslintrc.cjs",
    ".gitignore",
    ".lesrc",
    "ava.config.js",
    "LICENSE"
  ],
  "keywords": [
    "les",
    "koajs",
    "server",
    "cli",
    "expressjs"
  ],
  "author": "Richard Schloss",
  "license": "MIT",
  "dependencies": {
    "glob": "^7.1.6",
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "les-utils": "file:../les-utils",
    "minimist": "^1.2.5",
    "node-netstat": "^1.8.0",
    "socket.io": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.6.0",
    "eslint": "^7.22.0",
    "nodemon": "^2.0.7"
  },
  "imports": {
    "#@/*": "./*"
  }
}
